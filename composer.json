{
  "name": "wtysl",
  "type": "project",
  "license": "",
  "description": "WTYSL WordPress theme",
  "homepage": "http://whattookyousolong.org",
  "config": {
    "preferred-install": "dist",
    "generate-salts": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4",
    "composer/installers": "~1.0.12",
    "wpackagist-plugin/post-type-archive-links": "~1.3.1",
    "wpackagist-plugin/advanced-custom-fields": "~4.4.2",
    "wpackagist-plugin/regenerate-thumbnails": "~2.2.4"
  },
  "extra": {
    "installer-paths": {
      "wordpress/wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
    }
  }
}
